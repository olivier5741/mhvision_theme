<!DOCTYPE html>
<head>
  <meta name="description" content="{{ site.description }}" />
  <meta name="author" content="{{ site.title }}" />
  <meta name="keywords" content="{{ site.keywords }}" />
  <koken:title />
  <koken:settings />
</head>
<body>
<header>
  <h1>{{ site.title }}</h1> 
  <nav>
    <koken:navigation />
  </nav>
</header>
<koken:load>
  <h2>{{ album.title }}</h2>
  {{ album.description paragraphs="true" }} 
  <ol class="grid">
    <koken:loop>
      <li>
		
		<div>
	  
		{{ content.title }}
		
		<koken:has_category title="Demetz">
			<p class="brand">Demetz</p>
		<koken:else>						
			<koken:has_category title="Alpina">
				<p class="brand">Alpina</p>
			<koken:else>						
				<koken:has_category title="Coozy">
					<p class="brand">Coozy</p>
				<koken:else>						
					<p class="brand"><br /></p>					
				</koken:has_category>					
			</koken:has_category>				
		</koken:has_category>
	  
		<koken:has_category title="Homme">				
			<koken:has_category title="Femme">
				<p>Homme/Femme</p>
			<koken:else>						
				<p>Homme</p>					
			</koken:has_category>			  
		<koken:else>	
			<koken:has_category title="Femme">			  
				<p>Femme<p>
			<koken:else>		
				<koken:has_category title="Enfant">			  
					<p>Enfant<p>
				<koken:else>		
					<koken:has_category title="Junior">			  
						<p>Junior<p>
					<koken:else>
						<p>
						<br />
						</p>
					</koken:has_category>
				</koken:has_category>
			</koken:has_category>		
		</koken:has_category>	
		
        <koken:link>
          <koken:img />
        </koken:link>	
		
		<koken:has_category title="Kit Correction Optique">
		  <p>Kit Correction Optique</p>
		<koken:else>
			<p> </p>
		</koken:has_category>
		
		<koken:has_category title="Cordon">
		  <p>Cordon</p>
		<koken:else>
			<p> </p>
		</koken:has_category>
		
		</div>
		
      </li>
    </koken:loop>
  </ol>
<koken:else>
  <koken:note>
    No album data found 
  </koken:note>
</koken:load>
<footer>
</footer>
</body>
</html>