<!DOCTYPE html>
<head>
  <meta name="description" content="{{ site.description }}" />
  <meta name="author" content="{{ site.title }}" />
  <meta name="keywords" content="{{ site.keywords }}" />
  <koken:title />
  <koken:settings />
</head>
<body>
<header>
  <h1>{{ site.title }}</h1> 
  <nav>
    <koken:navigation />
  </nav>
</header>

<koken:load>
  <h2>{{ album.title }}</h2>
  {{ album.description paragraphs="true" }} 
  <ol class="grid">
    <koken:loop>
      <li>
		
		<div>
	  
		{{ content.title }}
		
		
		<koken:has_category title="Alpina">
			<p class="brand">Alpina</p>
		<koken:else>						
			<koken:has_category title="Bausch and Lomb">
				<p class="brand">Bausch and Lomb</p>
			<koken:else>						
				<koken:has_category title="Bollé Safety">
					<p class="brand">Bollé Safety</p>
				<koken:else>								
					<koken:has_category title="Ciba Vision">
						<p class="brand">Ciba Vision</p>
					<koken:else>						
						<koken:has_category title="Coozy">
							<p class="brand">Coozy</p>
						<koken:else>						
							<koken:has_category title="Demetz">
								<p class="brand">Demetz</p>
							<koken:else>						
								<koken:has_category title="Dutz">
									<img class="brand" src="../dev/storage/own_image/dutz.png" class="brand" alt="Dutz"/>
								<koken:else>								
									<koken:has_category title="Peltor">
										<p class="brand">Peltor</p>
									<koken:else>						
										<koken:has_category title="Rec Specs">
											<p class="brand">Rec Specs</p>
										<koken:else>						
											<koken:has_category title="Titmus">
												<p class="brand">Titmus</p>
											<koken:else>		
												<pclass="brand"><br/></p>					
											</koken:has_category>					
										</koken:has_category>					
									</koken:has_category>					
								</koken:has_category>				
							</koken:has_category>					
						</koken:has_category>					
					</koken:has_category>					
				</koken:has_category>					
			</koken:has_category>				
		</koken:has_category>
	  
		<koken:has_category title="Homme">				
			<koken:has_category title="Femme">
				<p>Homme/Femme</p>
			<koken:else>						
				<p>Homme</p>					
			</koken:has_category>			  
		<koken:else>	
			<koken:has_category title="Femme">			  
				<p>Femme<p>
			<koken:else>		
				<koken:has_category title="Enfant">			  
					<p>Enfant<p>
				<koken:else>		
					<koken:has_category title="Junior">			  
						<p>Junior<p>
					<koken:else>
						<p><br /></p>
					</koken:has_category>
				</koken:has_category>
			</koken:has_category>		
		</koken:has_category>	
		
		</div>
		
        <koken:link>
          <koken:img />
        </koken:link>	
		
		<koken:has_category title="Kit Correction Optique">
		  <p>Kit Correction Optique</p>
		<koken:else>
		</koken:has_category>
		
		<koken:has_category title="Cordon">
		  <p>Cordon</p>
		<koken:else>
		</koken:has_category>
		
      </li>
    </koken:loop>
  </ol>
<koken:else>
  <koken:note>
    Pas d'album trouvé
  </koken:note>
</koken:load>
<footer>
</footer>
</body>
</html>